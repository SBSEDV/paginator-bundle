{
    "name": "sbsedv/paginator-bundle",
    "description": "A Symfony bundle that streamlines pagination with Doctrine ORM.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Burger",
            "homepage": "https://github.com/danielburger1337"
        }
    ],
    "autoload": {
        "psr-4": {
            "SBSEDV\\Bundle\\PaginatorBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^2.10",
        "symfony/framework-bundle": "^6.1",
        "symfony/serializer": "^6.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-symfony": "^1.1",
        "symfony/web-link": "^6.1"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
